<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_albums">

    <fragment
        android:id="@+id/navigation_albums"
        android:name="com.apps.kunalfarmah.echo.Online.ui.albums.AlbumsFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_albums" />

    <fragment
        android:id="@+id/navigation_artists"
        android:name="com.apps.kunalfarmah.echo.Online.ui.artists.ArtistsFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/fragment_artists" />

    <fragment
        android:id="@+id/navigation_tracks"
        android:name="com.apps.kunalfarmah.echo.Online.ui.tracks.TracksFragment"
        android:label="@string/title_notifications"
        tools:layout="@layout/fragment_tracks" />
</navigation>
